[32m+[m[32mfunction enviar() {[m
[32m+[m[32m  let nome = document.getElementById("nomesobrenome").value;[m
[32m+[m[32m  let email = document.getElementById("email").value;[m
[32m+[m[32m  let tel = document.getElementById("telefone").value;[m
[32m+[m[32m  let contatos = {[m
[32m+[m[32m    nome: nome,[m
[32m+[m[32m    email: email,[m
[32m+[m[32m    tel: tel,[m
[32m+[m[32m  };[m
[32m+[m[32m  let nossoContato = document.querySelector([m
[32m+[m[32m    'input[name="contato"]:checked'[m
[32m+[m[32m  ).value;[m
[32m+[m
[32m+[m[32m  var select = document.getElementById("turno");[m
[32m+[m[32m  var turno = select.options[select.selectedIndex].value;[m
[32m+[m
[32m+[m[32m  if ([m
[32m+[m[32m    (contatos.nome != "" && contatos.nome != undefined,[m
[32m+[m[32m    contatos.email != "" && contatos.email != undefined,[m
[32m+[m[32m    contatos.tel != "" && contatos.tel != undefined)[m
[32m+[m[32m  ) {[m
[32m+[m[32m    confirm([m
[32m+[m[32m      "Nome: " +[m
[32m+[m[32m        contatos.nome +[m
[32m+[m[32m        "\n" +[m
[32m+[m[32m        "e-mail: " +[m
[32m+[m[32m        contatos.email +[m
[32m+[m[32m        "\n" +[m
[32m+[m[32m        "Tel.: " +[m
[32m+[m[32m        contatos.tel +[m
[32m+[m[32m        "\n" +[m
[32m+[m[32m        "Turno √† ser atendido: " +[m
[32m+[m[32m        turno +[m
[32m+[m[32m        "\n" +[m
[32m+[m[32m        "\n" +[m
[32m+[m[32m        "Entraremos em contato atrav√©s do seu " +[m
[32m+[m[32m        nossoContato +[m
[32m+[m[32m        " para confirmar seu hor√°rio." +[m
[32m+[m[32m        "\n" +[m
[32m+[m[32m        "Obs: Caso seus dados n√£o estajam corretos aperte CANCELAR ou OK para confirmar."[m
[32m+[m[32m    );[m
[32m+[m[32m  } else {[m
[32m+[m[32m    alert("Favor preencher os campos obrigat√≥rios!");[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
